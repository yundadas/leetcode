class Solution:
    def countEven(self, num: int) -> int:
        out=0
        for i in range(1,num+1):
            if self.is_digit_sum_even(i):
                out += 1
        return out 

     
    def is_digit_sum_even(self, i):
        out = 0
        while i:
            i,residue = i//10, i%10
            out += residue
        return not(out%2)
